{
  "gitbook": "3.2.3",
  "plugins": [
    "favicon",
    "autotheme", "-lunr", "-search",
    "search-plus",
    "prism", "-highlight",
    "splitter",
    "-sharing",
    "tbfed-pagefooter",
    "expandable-chapters-small",
    "book-summary-scroll-position-saver",
    "anchor-navigation-ex",
    "copy-code-button",
    "multipart",
    "simple-page-toc",
    "page-treeview",
    "-mygitalk",
    "popup"
  ],
  "pluginsConfig": {
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "theme-default": {
      "showLevel": false
    },
    "fontSettings": {
      "family": "serif",
      "size": 2
    },
    "github": {
      "url": "https://github.com/YYETST/customer_success_doc"
    },
    "autotheme": {
      "white": [9, 10, 11, 12, 13, 14, 15, 16, 17],
      "sepia": [9, 10, 11, 12, 13, 14, 15, 16, 17],
      "night": [9, 10, 11, 12, 13, 14, 15, 16, 17]
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy 用友 -【客户成功部】【生态技术部】 2021",
      "modify_label": "修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "page-treeview": {
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": false,
      "printLog": false,
      "multipleH1": true,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fa fa-navicon",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "favicon": {
      "shortcut": "/assets/images/favicon.ico",
      "bookmark": "/assets/images/favicon.ico",
      "appleTouch": "/assets/images/apple-touch-icon.png",
      "appleTouchMore": {
        "120x120": "/assets/images/apple-touch-icon-120x120.png",
        "180x180": "/assets/images/apple-touch-icon-180x180.png"
      }
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontFamily": "Arial",
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  },
  "title": "【客户成功部】【生态技术部】",
  "description": "用友-【客户成功部】【生态技术部】 联合推出",
  "author": "王立",
  "language": "zh-hans",
  "root": ".",
  "structure": {
    "readme": "README.md"
  }
}